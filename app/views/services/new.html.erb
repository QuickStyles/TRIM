<div class="col-md-6">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">Create a new service</h3>
    </div>
    <div class="panel-body">
      <%= simple_form_for @service do |s| %>
      <%= s.input :name%>
      <%= s.input :rate %>
      <%= s.input :location %>
      <%= s.input :duration %>
      <%= s.button :submit %>
      <% end %>
    </div>
  </div>
</div>

